{
    "name": "vue-fsd",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:prod": "vue-tsc && vite build --mode production",
        "build:dev": "vue-tsc && vite build --mode development",
        "preview": "vite preview",
        "lint:ts": "eslint \"**/*.{vue,ts,tsx}\"",
        "lint:ts-fix": "eslint \"**/*.{vue,ts,tsx}\" --fix",
        "lint:styles": "npx stylelint \"**/*.{css,scss,vue}\"",
        "lint:styles-fix": "npx stylelint \"**/*.{css,scss,vue}\" --fix",
        "prettier:check": "npx prettier --check .",
        "prettier:fix": "npx prettier --write .",
        "test:unit": "vitest --config config/vitest/vitest.config.ts",
        "test:unit-interface": "vitest --ui --config config/vitest/vitest.config.ts",
        "test:unit:watch": "vitest watch --config config/vitest/vitest.config.ts",
        "test:unit:coverage": "vitest run --coverage --config config/vitest/vitest.config.ts",
        "test:ui": "npx loki test",
        "test:ui:ok": "npx loki approve",
        "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "storybook": "start-storybook -p 6006 -c ./config/storybook --no-open",
        "storybook:build": "build-storybook -c ./config/storybook"
    },
    "dependencies": {
        "i18next": "^22.4.10",
        "i18next-browser-languagedetector": "^7.0.1",
        "i18next-http-backend": "^2.1.1",
        "i18next-vue": "^2.1.1",
        "vue": "^3.2.45",
        "vue-error-boundary": "^2.0.1",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-interactions": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/builder-vite": "^0.4.2",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/vue3": "^6.5.16",
        "@testing-library/vue": "^6.6.1",
        "@types/node": "^18.11.18",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.28.5",
        "@vitest/ui": "^0.29.1",
        "@vue/test-utils": "^2.3.0",
        "babel-loader": "^8.3.0",
        "dotenv": "^16.0.3",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-storybook": "^0.6.11",
        "eslint-plugin-vue": "^9.9.0",
        "jsdom": "^21.1.0",
        "loki": "^0.31.0",
        "postcss-html": "^1.5.0",
        "prettier": "2.8.4",
        "reg-cli": "^0.17.7",
        "sass": "^1.58.0",
        "storybook-vue3-router": "^2.3.1",
        "stylelint": "^15.2.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard-scss": "^7.0.1",
        "typescript": "^4.9.3",
        "vite": "^4.1.0",
        "vite-plugin-checker": "^0.5.6",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-stylelint": "^4.2.0",
        "vite-svg-loader": "^4.0.0",
        "vite-tsconfig-paths": "^4.0.5",
        "vitest": "^0.28.5",
        "vue-loader": "^16.8.3",
        "vue-tsc": "^1.2.0"
    },
    "loki": {
        "diffingEngine": "looks-same",
        "looks-same": {
            "ignoreCaret": true
        },
        "chromeTolerance": false,
        "configurations": {
            "chrome.laptop": {
                "target": "chrome.docker",
                "width": 1366,
                "height": 768,
                "deviceScaleFactor": 1,
                "mobile": false
            },
            "chrome.iphone7": {
                "target": "chrome.docker",
                "preset": "iPhone 7"
            }
        }
    }
}
